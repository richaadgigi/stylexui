{
  "name": "@richaadgigi/stylexui",
  "version": "2.0.10",
  "description": "Build responsive, beautiful interfaces faster than ever with utility-first classes and smart defaults. No bloat. No fuss. Just results.",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.css",
    "*.js",
    "*.d.ts",
    "css/",
    "assets/",
    "js/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:xui-js": "uglifyjs js/xui.js --compress --mangle --output js/xui.min.js",
    "build:js": "uglifyjs index.js --compress --mangle --output dist/index.min.js",
    "build:css": "cleancss css/xui.css -O2 -o css/xui.min.css",
    "generate": "node generator/generate.js",
    "bundle:css": "node scripts/bundle.js",
    "build": "npm run clean && mkdirp dist && npm run build:js && npm run build:xui-js && npm run build:css",
    "prepublishOnly": "npm run build",
    "build:xui": "node generator/generate.js",
    "dev:watch": "node watcher.js"
  },
  "exports": {
    ".": "./index.js",
    "./css/xui.css": "./css/xui.css",
    "./css/xui.min.css": "./css/xui.min.css"
  },
  "sideEffects": [
    "build/xui-utilities.css",
    "css/xui.min.css"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richaadgigi/stylexui.git"
  },
  "keywords": [
    "css",
    "xui",
    "style",
    "js",
    "stylexui",
    "ui-framework",
    "frontend",
    "css-framework",
    "javascript-library",
    "xnyder"
  ],
  "author": "Richard Gigi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richaadgigi/stylexui/issues"
  },
  "homepage": "https://github.com/richaadgigi/stylexui#readme",
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "chokidar-cli": "^3.0.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "postcss": "^8.5.6",
    "webpack-virtual-modules": "^0.6.2"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "cpy-cli": "^5.0.0",
    "mkdirp": "^3.0.1",
    "onchange": "^7.1.0",
    "rimraf": "^5.0.10",
    "uglify-js": "^3.19.3"
  }
}